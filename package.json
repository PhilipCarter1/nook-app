{
  "name": "nook-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:simple": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "deploy": "npm run build:simple && npx vercel --prod",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:comprehensive": "./tests/e2e/run-comprehensive-tests.sh",
    "test:e2e:tenant": "./tests/e2e/run-comprehensive-tests.sh tenant",
    "test:e2e:landlord": "./tests/e2e/run-comprehensive-tests.sh landlord",
    "test:e2e:admin": "./tests/e2e/run-comprehensive-tests.sh admin",
    "test:e2e:auth": "./tests/e2e/run-comprehensive-tests.sh auth",
    "test:e2e:features": "./tests/e2e/run-comprehensive-tests.sh features",
    "test:e2e:ui-ux": "./tests/e2e/run-comprehensive-tests.sh ui",
    "test:e2e:platform": "./tests/e2e/run-comprehensive-tests.sh platform",
    "test:e2e:headed": "HEADLESS=false ./tests/e2e/run-comprehensive-tests.sh",
    "test:e2e:firefox": "BROWSER=firefox ./tests/e2e/run-comprehensive-tests.sh",
    "test:e2e:webkit": "BROWSER=webkit ./tests/e2e/run-comprehensive-tests.sh",
    "test:e2e:fast": "WORKERS=1 TIMEOUT=15000 ./tests/e2e/run-comprehensive-tests.sh",
    "test:e2e:slow": "TIMEOUT=60000 RETRIES=3 ./tests/e2e/run-comprehensive-tests.sh",
    "backup": "ts-node scripts/backup.ts",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:migrate": "tsx scripts/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:setup": "tsx scripts/setup-db.ts",
    "db:backup": "tsx scripts/backup.ts",
    "test:security": "vitest run security.test.ts"
  },
